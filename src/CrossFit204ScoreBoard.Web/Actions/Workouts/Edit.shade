viewdata model="CrossFit204ScoreBoard.Web.Actions.Workouts.EditWorkoutViewModel"

formfor model="EditWorkoutViewModel"
	edit property="Workout.Name"
	br
	edit property="Workout.Description"
	br
	edit property="Workout.TrackTime"
	br
	edit property="Workout.TrackWeight"
	br
	edit property="Workout.TrackReps"
	br
	edit property="Workout.TrackRounds"
	br
	inputfor property="Workout.Id"
	input.btn.btn-inverse type="submit" value="Update Workout"