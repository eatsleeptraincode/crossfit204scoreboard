viewdata model="CrossFit204ScoreBoard.Web.Actions.ScoreBoardViewModel"


ul
	li each="var i in Model.Items"
		=i.WorkoutName
		p |Men
		ol
			li each="var s in i.MenScores"
				=s.Athlete.FirstName + " " + s.Athlete.LastName
				=s.Weight
		p |Women
		ol
			li each="var s in i.WomenScores"
				=s.Athlete.FirstName + " " + s.Athlete.LastName				
				=s.Weight

		

