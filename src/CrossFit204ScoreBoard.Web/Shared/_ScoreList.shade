ol
	.scorelist
		li each="var s in scores"
			.whitelink
				=this.LinkTo(new AthleteDetailsRequest{AthleteId = s.Athlete.Id.Replace("athletes/","")}).Text(s.Athlete.FirstName + " " + s.Athlete.LastName)
				|&nbsp;&nbsp;
				test if="s.Workout.TrackWeight"
					=s.Weight
				test if="s.Workout.TrackTime"
					=s.Time.Minutes
					|:
					=s.Time.Seconds
				test if="s.Workout.TrackRounds"
					s.Rounds
				test if="s.Workout.TrackReps"
					s.Reps